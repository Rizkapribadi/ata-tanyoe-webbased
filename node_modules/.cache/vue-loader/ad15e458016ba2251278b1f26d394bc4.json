{"remainingRequest":"D:\\laragon\\www\\kuliner\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!D:\\laragon\\www\\kuliner\\src\\views\\Keranjang.vue?vue&type=script&lang=js&","dependencies":[{"path":"D:\\laragon\\www\\kuliner\\src\\views\\Keranjang.vue","mtime":1638335020206},{"path":"D:\\laragon\\www\\kuliner\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"D:\\laragon\\www\\kuliner\\node_modules\\babel-loader\\lib\\index.js","mtime":315532800000},{"path":"D:\\laragon\\www\\kuliner\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"D:\\laragon\\www\\kuliner\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["Keranjang.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA+FA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Keranjang.vue","sourceRoot":"src/views","sourcesContent":["<template>\r\n  <div class=\"keranjang\">\r\n    <Navbar :updateKeranjang=\"keranjangs\" />\r\n    <div class=\"container\">\r\n      <!-- breadcrumb -->\r\n      <div class=\"row mt-5\">\r\n        <div class=\"col\">\r\n          <nav aria-label=\"breadcrumb\">\r\n            <ol class=\"breadcrumb\">\r\n              <li class=\"breadcrumb-item\">\r\n                <router-link to=\"/\" class=\"text-dark\">Home</router-link>\r\n              </li>\r\n              <li class=\"breadcrumb-item\">\r\n                <router-link to=\"/foods\" class=\"text-dark\">Foods</router-link>\r\n              </li>\r\n              <li class=\"breadcrumb-item active\" aria-current=\"page\">\r\n                Keranjang\r\n              </li>\r\n            </ol>\r\n          </nav>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"row\">\r\n        <div class=\"col\">\r\n          <h2>Keranjang <strong>Saya</strong></h2>\r\n          <div class=\"table-responsive mt-3\">\r\n            <table class=\"table\">\r\n              <thead>\r\n                <tr>\r\n                  <th scope=\"col\">#</th>\r\n                  <th scope=\"col\">Foto</th>\r\n                  <th scope=\"col\">Nama</th>\r\n                  <th scope=\"col\">Keterangan</th>\r\n                  <th scope=\"col\">Jumlah</th>\r\n                  <th scope=\"col\">Harga</th>\r\n                  <th scope=\"col\">Total Harga</th>\r\n                  <th scope=\"col\">Hapus</th>\r\n                </tr>\r\n              </thead>\r\n              <tbody>\r\n                <tr\r\n                  v-for=\"(keranjang, index) in keranjangs\"\r\n                  :key=\"keranjang.id\"\r\n                >\r\n                  <th>{{ index + 1 }}</th>\r\n                  <td>\r\n                    <img\r\n                      :src=\"'../assets/images/' + keranjang.products.gambar\"\r\n                      class=\"img-fluid shadow\"\r\n                      width=\"250\"\r\n                    />\r\n                  </td>\r\n                  <td>\r\n                    <strong>{{ keranjang.products.nama }}</strong>\r\n                  </td>\r\n                  <td>\r\n                    {{ keranjang.keterangan ? keranjang.keterangan : \"-\" }}\r\n                  </td>\r\n                  <td>\r\n                    {{ keranjang.jumlah_pesanan }}\r\n                  </td>\r\n                  <td align=\"right\">Rp. {{ keranjang.products.harga }}</td>\r\n                  <td align=\"right\">\r\n                    <strong>\r\n                      Rp.\r\n                      {{ keranjang.products.harga * keranjang.jumlah_pesanan }}\r\n                    </strong>\r\n                  </td>\r\n                  <td align=\"center\" class=\"text-danger\">\r\n                    <b-icon-trash\r\n                      @click=\"hapusKeranjang(keranjang.id)\"\r\n                    ></b-icon-trash>\r\n                  </td>\r\n                </tr>\r\n\r\n                <tr>\r\n                  <td colspan=\"6\" align=\"right\">\r\n                    <strong>Total Harga : </strong>\r\n                  </td>\r\n                  <td align=\"right\">\r\n                    <strong>Rp. {{ totalHarga }}</strong>\r\n                  </td>\r\n                  <td></td>\r\n                </tr>\r\n              </tbody>\r\n            </table>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Navbar from \"@/components/Navbar.vue\";\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"Keranjang\",\r\n  components: {\r\n    Navbar,\r\n  },\r\n  data() {\r\n    return {\r\n      keranjangs: [],\r\n    };\r\n  },\r\n  methods: {\r\n    setKeranjang(data) {\r\n      this.keranjangs = data;\r\n    },\r\n    hapusKeranjang(id) {\r\n      axios\r\n        .delete(\"http://localhost:3000/keranjangs/\" + id)\r\n        .then(() => {\r\n          this.$toast.error(\"Item Berhasil Dihapus\", {\r\n            type: \"error\",\r\n            position: \"top-right\",\r\n            duration: 3000,\r\n            dismissible: true,\r\n          });\r\n\r\n          //update data keranjang\r\n          axios\r\n            .get(\"http://localhost:3000/keranjangs\")\r\n            .then((response) => this.setKeranjang(response.data))\r\n            .catch((error) => console.log(error));\r\n        })\r\n        .catch((error) => console.log(error));\r\n    },\r\n  },\r\n  mounted() {\r\n    axios\r\n      .get(\"http://localhost:3000/keranjangs\")\r\n      .then((response) => this.setKeranjang(response.data))\r\n      .catch((error) => console.log(error));\r\n  },\r\n  computed: {\r\n    totalHarga() {\r\n      return this.keranjangs.reduce(function (items, data) {\r\n        return items + data.products.harga * data.jumlah_pesanan;\r\n      }, 0);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"]}]}